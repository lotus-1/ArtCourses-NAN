
{{#each courses}}
<div>
  <h3>{{this.course_name}}</h3>
  <img src={{this.img_url}} alt={{this.alt_name}}>
  <h5>WHEN?</h5>
  <p>{{this.timewhen}}</p>
  <h5>WHERE?</h5>
  <p>{{this.place}}</p>
  <h5>AGE?</h5>
  <p>{{this.age}}</p>
  <h5>HOW MUCH?</h5>
  <p>{{this.cost}}</p>
  <button type="submit" name="join"> Join Course</button>
  <button type="submit" name="cancel"> Cancel</button>
</div>
{{/each}}
